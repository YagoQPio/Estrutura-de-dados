#include <stdio.h>
#include <stdlib.h>
/////Declaracao de variaveis

int vetor[10000];
int i = 0;
int j = 0;

///Funcao principal de execucao do progama

int main(){
    //Inicializacao da memoria
    for (i = 0; i < 10000; i++){
        vetor[i] = (rand() % 10000);

    }
    for (i = 0; i < 10000; i++){
        printf("%d\n", vetor[i]);
    }
    //Ordenacçao propriamente ditada
    for (i = 0 ; i < 9999 ; i++){// do primeiro ao penultimo
        for (j = (i + 1) ; j < 10000 ; j++){ // do proximo ao ultimo
             if (vetor[i] > vetor[j]){
                int temp = vetor[i];
                vetor[i] =  vetor[j];
                vetor[j] = temp;
             }
        }
    }
    for (i = 0; i < 10000; i++){
        printf("%d\n", vetor[i]);
    }
}
///////////////////////////////////////////////////////////////////////////
//Progamacao de ordenaçao em bubbleSort
#include <stdio.h>
#include <stdlib.h>
/////Declaracao de variaveis

int vetor[10000];
int i = 0;
int busca = 0;
int achei = 0;
///Funcao principal de execucao do progama

int main(){
    //Inicializacao da memoria
    for (i = 0; i < 10000; i++){
        vetor[i] = (rand() % 10000);
    }
    //Perguntar o parametro
    printf("Digie o valor a ser buscado: ");
    scanf("%d", &busca);

    //Busca propriamente dita
    achei = -1;
    for (i = 0; i < 10000; i++){
        if (vetor[i] == busca){
            achei = i;
        }
    }
    if (achei == -1){
        printf("Se fudeu! Nao existe");
    } else{
        printf("Achei Georges, Seu merda!!!! Na posiçao %d\n",achei);
    }
}


