#include <stdio.h>

//Declarar variavel
int vetor[10];
int i = 0;

//Funcao principal de execução do progama
int main(){

//inicializacao da memoria
    for (i = 0; i <10; i++){
        vetor[i] =  0;}

//Preenchimento do vetor
    for (i = 0; i <10; i++){
        printf("Digite o valor da posicao %d: ",i);
        scanf("%d", &vetor[i]);
    }
 // Impresao vetor
    for (i = 0; i <10; i++){
        printf("vetor[%d] =  %d\n", i, vetor[i]);
    }
}

///////////////////////////////////////////Inversor de vetor///////////////////////////////
#include <stdio.h>

// Progama de inversor de elementos do vetor
// Declarar Variaveis

int vetorOriginal[10];
int vetorInvertido[10];
int i = 0;

//Funcao principal da execucao do progama
int main(){
    //limpeza da memoria
    for (i = 0; i < 10; i ++){
        vetorOriginal[i] = 0;
        vetorInvertido[i] =0;
    }
    //preenchimento do vetor
    for (i = 0; i < 10; i ++){
        printf("Digite o valor da posicao %d do vetor: ",i);
        scanf("%d", &vetorOriginal[i]);
    }
    //Inversao do vetor
    for (i = 9; i>= 0; i--){
        vetorInvertido[9-i] = vetorOriginal[i];
    }
    //Impressao dos vetores
    for (i = 0; i < 10; i++){
        printf("vetorOriginal[%d] = %d\n", i , vetorOriginal[i]);
    }
    printf("\n");
    for (i = 0; i < 10; i++){
        printf("vetorInvertiso[%d] = %d\n", i , vetorInvertido[i]);
    }
}

////////////////////////////////////
//Progama de Trasposicao de Matriz//
////////////////////////////////////
#import <stdio.h>

//DecLaracao de variavel
int matriz[2][3];
int transposta[3][2];
int i;
int j;

//Funcao principal do progama
int main(){
//Limpeza da memoria
    for (i = 0 ; i < 2; i++){
        for (j = 0; j < 3; j++){
            matriz[i][j] = 0;
            transposta[j][i] = 0;
        }
        //preenchimento da matriz
         for (i = 0 ; i < 2; i++){
            for(j = 0 ; j < 3 ; j++){
                printf("matriz[%d, %d] = ", i,j);
                scanf("%d", &matriz[i][j]);
            }
        }
        //Transposicao
        for (i = 0 ; i < 2; i++){
            for(j = 0 ; j < 3 ; j++){
                transposta[j][i] =  matriz[i][j];
            }
    }
    //Imprimir matriz
    for (i = 0 ; i < 2; i++){
            for(j = 0 ; j < 3 ; j++){
                printf("%d\t", matriz[i][j]);
            }
            printf("\n");
    }
    printf("\n");
     for (i = 0 ; i < 3; i++){
            for(j = 0 ; j < 2 ; j++){
                printf("%d\t", transposta[i][j]);
            }
            printf("\n");
    }
}
}

