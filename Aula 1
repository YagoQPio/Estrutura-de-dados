#include <stdio.h>

//Declarar variavel
int vetor[10];
int i = 0;

//Funcao principal de execução do progama
int main(){

//inicializacao da memoria
    for (i = 0; i <10; i++){
        vetor[i] =  0;}

//Preenchimento do vetor
    for (i = 0; i <10; i++){
        printf("Digite o valor da posicao %d: ",i);
        scanf("%d", &vetor[i]);
    }
 // Impresao vetor
    for (i = 0; i <10; i++){
        printf("vetor[%d] =  %d\n", i, vetor[i]);
    }
}
